{
  "compilerOptions": {    
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "baseUrl": "./",
    "outDir": "./dist_compiledJS/",    
  },
  "exclude": [    
    "src_typescript/excludedTS"
  ],
  "include": [
    "src_typescript"
  ]
}
